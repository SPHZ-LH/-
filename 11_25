#include<stdio.h>
struct book
{
	char name[20];
	short price;



};
int main(void)
{
	struct book b1 = {"SPHZ百科全书",44};
	printf("价格：%d\n", b1.price);
	printf("书名：%s\n", b1.name);
	printf("书名：%s\n", b1.name);
	printf("价格：%d元\n", b1.price);


	return 0;
}
#include<stdio.h>
int main(void)
{
	int age = 20;
	if (age < 18)
		printf("未成年\n");
	else (age > 18);
		printf("成年\n");
	return 0;
}
